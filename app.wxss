/* 位置授权弹框 */
.location_cont {
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    z-index: 500;
    width: 540rpx;
    border-radius: 5rpx;
    -webkit-border-radius: 5rpx;
    -moz-border-radius: 5rpx;
    -o-border-radius: 5rpx;
    background: #fff;
    padding: 30rpx;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -o-box-sizing: border-box;
}

.location_btn {
    width: 50%;
    line-height: 50rpx;
    margin: 20rpx auto 0;
    border: 1rpx solid #e9e9e9;
    font-size: 28rpx;
    font-weight: normal;
}

/* 滚动公告 */

.voice_icon {
    width: 40rpx;
    height: 40rpx;
}

.voice_icon1 {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    width: 60rpx;
    left: 0rpx;
    z-index: 100;
}

.yy_GongGao {
    height: 60rpx;
    line-height: 60rpx;
    font-size: 24rpx;
    text-indent: 60rpx;
    width: 100%;
    overflow: hidden;
}

.yy_marquee {
    width: 100%;
    position: relative;
}

.yy_marquee_text {
    white-space: nowrap;
    position: absolute;
    top: 0;
}

.fixed_top {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 2;
    margin: 0;
    box-shadow: 0 2px 2px rgba(0, 0, 0, 0.05);
    background: #fff;
    animation: fadeInDown 0.3s linear forwards;
    min-width: 100%;
}

@keyframes fadeInDown {
    0% {
        opacity: 0;
        transform: translateY(-100%);
    }

    100% {
        opacity: 1;
        transform: translateY(0);
    }
}

